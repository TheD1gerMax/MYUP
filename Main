package Main;

import java.util.Scanner;

public class Main
{

    public static void main(String [] args)
    {
        Scanner in = new Scanner(System.in);

        int n;


        n = in.nextInt();

        Product []products = new Product[n];
        for (int i = 0; i < n; i++)
        {
            products[i] = new Product();
            products[i].ProductConsoleIN();
        }

        for (int i = 0; i < n; i++)
        {
            products[i].ProductConsoleOut();
        }
    }
}
